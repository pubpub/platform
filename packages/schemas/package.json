{
	"name": "schemas",
	"type": "module",
	"version": "0.0.1",
	"exports": {
		".": "./dist/schemas.js",
		"./utils": "./dist/schemas-utils.js",
		"./formats": "./dist/schemas-formats.js",
		"./schemas": "./dist/schemas-schemas.js",
		"./package.json": "./package.json"
	},
	"scripts": {
		"type-check": "tsc --noEmit",
		"type-check:go": "tsgo --noEmit",
		"test": "vitest run"
	},
	"dependencies": {
		"@sinclair/typebox": "catalog:",
		"db": "workspace:*",
		"lucide-react": "^0.469.0",
		"zod": "catalog:"
	},
	"devDependencies": {
		"react": "catalog:react19",
		"tsconfig": "workspace:*",
		"typescript": "catalog:",
		"@typescript/native-preview": "catalog:",
		"vitest": "catalog:"
	},
	"preconstruct": {
		"entrypoints": ["index.ts", "schemas.ts", "formats.ts", "utils.ts"],
		"exports": true,
		"___experimentalFlags_WILL_CHANGE_IN_PATCH": {
			"typeModule": true,
			"distInRoot": true,
			"importsConditions": true
		}
	}
}
