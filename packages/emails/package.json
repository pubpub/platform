{
	"name": "emails",
	"type": "module",
	"version": "1.0.0",
	"main": "dist/emails.cjs.js",
	"module": "dist/emails.esm.js",
	"exports": {
		".": "./dist/emails.js",
		"./package.json": "./package.json"
	},
	"private": true,
	"scripts": {
		"dev": "cp .env.development ./node_modules/react-email/ && pnpm email dev -d src -p 3003",
		"type-check": "tsc --noEmit",
		"format": "prettier --check . --ignore-path ../../.gitignore",
		"format:fix": "prettier -w . --ignore-path ../../.gitignore",
		"lint": "eslint **/*.tsx",
		"lint:fix": "eslint --fix **/*.tsx",
		"export": "email export"
	},
	"devDependencies": {
		"@pubpub/eslint-config": "workspace:*",
		"@pubpub/prettier-config": "workspace:*",
		"@types/node": "catalog:",
		"@types/react": "catalog:react19",
		"dotenv-cli": "^7.4.4",
		"react": "catalog:react19",
		"tsconfig": "workspace:*"
	},
	"dependencies": {
		"@react-email/components": "0.0.31",
		"browserslist": "^4.25.3",
		"db": "workspace:*",
		"eslint": "catalog:",
		"react": "catalog:react19",
		"react-email": "3.0.4",
		"tsconfig": "workspace:*",
		"typescript": "catalog:"
	},
	"preconstruct": {
		"entrypoints": [
			"index.tsx"
		],
		"exports": true,
		"___experimentalFlags_WILL_CHANGE_IN_PATCH": {
			"typeModule": true,
			"distInRoot": true,
			"importsConditions": true
		}
	},
	"files": [
		"dist"
	],
	"eslintConfig": {
		"extends": [
			"@pubpub/eslint-config/base",
			"@pubpub/eslint-config/react"
		]
	},
	"prettier": "@pubpub/prettier-config"
}
