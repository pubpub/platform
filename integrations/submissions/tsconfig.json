{
	"extends": "tsconfig/nextjs.json",
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"~/*": ["./*"]
		},
		"strictNullChecks": true,
		"noImplicitAny": null,
		"strict": false,
		"tsBuildInfoFile": "node_modules/.cache/tsbuildinfo.json"
	},
	"include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "lib/env.mjs"],
	"exclude": ["node_modules"]
}
